{% block widget_choice_options %}
	{% spaceless %}
		{% for choice, label in options %}
			<option disabled="disabled" value="{{ choice }}"{% if _form_is_choice_selected(form, choice) %} selected="selected"{% endif %}>{{ label }}</option>
		{% endfor %}
	{% endspaceless %}
{% endblock widget_choice_options %}

{% block tutoravailable_widget %}
	{% spaceless %}
		<select {{ block('widget_attributes') }}>
			{% set options = choices %}
			{{ block('widget_choice_options') }}
		</select>
	{% endspaceless %}
{% endblock tutoravailable_widget %}
